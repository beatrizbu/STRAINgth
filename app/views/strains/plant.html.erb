

<% @strains.each do |strain| %>
    <div class="col-xs-12 .col-md-8" id="">
        <div class="show-strains">
          <%= strain["name"] %>
          <br>
        <%= image_tag strain["image"] %>
        </div>
    </div>
    <div class="col-xs-6 .col-md-8">
      <h1>Lineage:</h1>
    <% strain["lineage"].each do |k, v| %>
    <%= "#{k}" %>
    <br>
    <% end %>




      <h1>Description:</h1>
      <% strain["symptoms"].each do |k, v| %>
      <%= "#{k.capitalize} - #{v.to_f.round(2)}" %>
      <br>
      <% end %>

      <h1>Also Known As:</h1>
        <% a = strain["genetics"] %>
          <% b = a.values[0]  %>
          <%= "#{b.gsub(/\//, ",")}" unless b.eql?(false)%><br>



      <br>
    </div>
    <br>
  <% end %>
